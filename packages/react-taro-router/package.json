{
  "name": "@medux/react-taro-router",
  "version": "2.0.0-beta.3.14",
  "description": "The practice of @medux/core in taro",
  "repository": "https://github.com/wooline/medux/tree/master/packages/react-taro-router",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "dist/cjs/index.js",
  "tsSource": "src/",
  "types": "types/",
  "files": [
    "src/",
    "env/",
    "types/",
    "dist/"
  ],
  "scripts": {
    "prebuild": "yarn lint && rimraf ./dist ./types && yarn types",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "types": "tsc --project ./types.tsconfig.json && copyfiles -f ./src/*.d.ts ./types",
    "build": "cross-env NODE_ENV=development NODE_TAG=pkg rollup -c & cross-env NODE_ENV=production NODE_TAG=pkg rollup -c"
  },
  "externals": {
    "@tarojs/taro": true,
    "react": true,
    "react-redux": true
  },
  "peerDependencies": {
    "@tarojs/taro": "^3.0.0",
    "@types/react-redux": "~7.0.0",
    "react-redux": "~7.0.0"
  },
  "dependencies": {
    "@medux/core": "^2.0.0-beta.3.14",
    "@medux/route-web": "^2.0.0-beta.3.14",
    "@medux/route-mp": "^2.0.0-beta.3.14"
  },
