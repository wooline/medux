{
  "name": "@medux/route-plan-a",
  "version": "0.1.2",
  "description": "The practice of @medux/core in web route",
  "repository": "https://github.com/wooline/medux/tree/master/packages/route-plan-a",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "tsSource": "src/",
  "types": "types/",
  "files": [
    "src/",
    "types/",
    "dist/",
    "index.js"
  ],
  "scripts": {
    "prebuild": "yarn lint && rimraf ./dist ./types && yarn types  && yarn test",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "types": "tsc && tsc --project ./types.tsconfig.json && copyfiles -f ./src/*.d.ts ./types",
    "build:cjs": "cross-env NODE_ENV=cjs babel ./src --ignore **/__tests__/,**/*.test.ts --out-dir ./dist/cjs  --extensions \".ts,.tsx\" --source-maps",
    "build:es": "cross-env NODE_ENV=es babel ./src --ignore **/__tests__/,**/*.test.ts --out-dir ./dist/es  --extensions \".ts,.tsx\" --source-maps",
    "build": "yarn build:cjs & yarn build:es",
    "test": "cross-env NODE_ENV=cjs jest"
  },
  "dependencies": {
    "@medux/web": "^0.1.2",
    "deep-extend": "^0.6.0",
    "path-to-regexp": "^3.0.0"
  },
  "gitHead": "c2d8bb9ed8d10102413bdf655c7326e94f4a4464"
}
