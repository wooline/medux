{
  "name": "@medux/route-plan-a",
  "version": "1.1.1-alpha.5",
  "description": "The practice of @medux/core in web route.",
  "repository": "https://github.com/wooline/medux/tree/master/packages/route-plan-a",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "jsnext": "dist/next/index.js",
  "tsSource": "src/",
  "types": "types/",
  "files": [
    "src/",
    "types/",
    "dist/"
  ],
  "scripts": {
    "type": "tsc",
    "typeSrc": "tsc -P ./src",
    "typeTests": "tsc -P ./tests",
    "eslint": "eslint --cache .",
    "eslintSrc": "eslint --cache ./src",
    "eslintTests": "eslint --cache ./tests",
    "buildTypes": "tsc --project ./src/tsconfig.types.json && copyfiles -f ./src/*.d.ts ./types",
    "test": "jest",
    "prebuild": "node ../../preBuild && yarn type & yarn typeSrc & yarn typeTests & yarn eslint & yarn eslintSrc & yarn eslintTests && yarn test && rimraf ./dist ./types && yarn buildTypes",
    "build:next": "cross-env NODE_ENV=production NODE_TAG=next babel ./src --out-dir ./dist/next  --extensions \".ts,.tsx\"",
    "build:esm": "cross-env NODE_ENV=production NODE_TAG=esm babel ./src --out-dir ./dist/esm  --extensions \".ts,.tsx\"",
    "build:cjs": "cross-env NODE_ENV=production NODE_TAG=cjs babel ./src --out-dir ./dist/cjs  --extensions \".ts,.tsx\"",
    "build": "yarn build:next & yarn build:esm & yarn build:cjs",
    "docs-md": "typedoc --plugin typedoc-plugin-markdown  --out api src/index.ts --hideSources --theme markdown && node fixDocs"
  },
  "dependencies": {
    "@babel/runtime": "7.9.6",
    "@medux/core": "^1.1.1-alpha.5"
  },
  "gitHead": "c2d8bb9ed8d10102413bdf655c7326e94f4a4464"
}
