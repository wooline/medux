{
  "name": "@medux/route-plan-a",
  "version": "0.2.30",
  "description": "The practice of @medux/core in web route.",
  "repository": "https://github.com/wooline/medux/tree/master/packages/route-plan-a",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "jsnext": "dist/next/index.js",
  "tsSource": "src/",
  "types": "types/",
  "files": [
    "src/",
    "types/",
    "dist/"
  ],
  "scripts": {
    "prebuild": "yarn lint && rimraf ./dist ./types && yarn types && yarn test",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "types": "tsc --project ./types.tsconfig.json && copyfiles -f ./src/*.d.ts ./types",
    "build:next": "cross-env NODE_ENV=next babel ./src --out-dir ./dist/next  --extensions \".ts,.tsx\"",
    "build:esm": "cross-env NODE_ENV=esm babel ./src --out-dir ./dist/esm  --extensions \".ts,.tsx\"",
    "build": "yarn build:next & yarn build:esm",
    "test": "jest"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.0.0"
  },
  "dependencies": {
    "@medux/core": "^0.2.30",
    "deep-extend": "~0.6.0"
  },
  "gitHead": "c2d8bb9ed8d10102413bdf655c7326e94f4a4464"
}
