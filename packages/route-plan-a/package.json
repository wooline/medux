{
  "name": "@medux/route-plan-a",
  "version": "0.1.0-alpha.2",
  "description": "The practice of @medux/core in web route",
  "repository": "https://github.com/wooline/medux/tree/master/packages/route-plan-a",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "index.js",
  "tsSource": "src/",
  "types": "types/",
  "files": [
    "src/",
    "types/",
    "dist/",
    "index.js"
  ],
  "scripts": {
    "prebuild": "yarn lint && rimraf ./dist ./types && yarn types",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "types": "tsc && tsc --emitDeclarationOnly --declaration --isolatedModules false --noEmit false  --outDir ./types && copyfiles -f ./src/*.d.ts ./types",
    "build:prod": "cross-env NODE_ENV=production babel ./src --out-dir ./dist/prod  --extensions \".ts,.tsx\" --source-maps",
    "build:dev": "cross-env NODE_ENV=development babel ./src --out-dir ./dist/dev  --extensions \".ts,.tsx\" --source-maps",
    "build": "yarn build:dev & yarn build:prod",
    "test": "jest"
  },
  "dependencies": {
    "@medux/web": "^0.1.0-alpha.2",
    "deep-extend": "^0.6.0",
    "path-to-regexp": "^3.0.0"
  },
  "gitHead": "d72a594493f4a076297491bcc925e9b7fcfbbe37"
}
