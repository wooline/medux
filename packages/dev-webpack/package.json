{
  "name": "@medux/dev-webpack",
  "version": "1.0.2",
  "description": "Common development tools",
  "repository": "https://github.com/wooline/medux/tree/master/packages/dev-webpack",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "eslintSrc": "../../node_modules/.bin/eslint --cache ./src",
    "clearDist": "../../node_modules/.bin/rimraf ./dist",
    "buildTypes": "tsc --project ./src/tsconfig.types.json",
    "prebuild": "yarn eslintSrc && yarn clearDist && yarn buildTypes",
    "build": "tsc -P ./src"
  },
  "peerDependencies": {
    "@medux/core": "^2.0.0"
  },
  "dependencies": {
    "nanoid": "~3.1.20",
    "schema-utils":"~3.0.0",
    "loader-utils": "~2.0.0",
    "typescript-json-schema": "0.43.0",
    "babel-loader": "~8.2.1",
    "css-loader": "~5.0.1",
    "file-loader": "~6.2.0",
    "url-loader": "~4.1.0",
    "raw-loader": "~4.0.0",
    "null-loader": "~4.0.0",
    "source-map-loader": "~1.1.2",
    "style-loader": "~2.0.0",
    "postcss-loader": "~4.0.4",
    "image-webpack-loader": "~7.0.1",
    "clean-webpack-plugin":"~3.0.0",
    "eslint-webpack-plugin":"~2.4.1",
    "stylelint-webpack-plugin": "~2.1.0",
    "mini-css-extract-plugin": "~1.3.0",
    "optimize-css-assets-webpack-plugin": "~5.0.3",
    "html-webpack-plugin": "next",
    "html-replace-webpack-plugin": "~2.5.5",
    "@pmmmwh/react-refresh-webpack-plugin": "~0.4.3",
    "webpack": "~5.11.0",
    "webpack-bundle-analyzer": "~4.1.0",
    "webpack-dev-server": "next"
  },
  "gitHead": "c2d8bb9ed8d10102413bdf655c7326e94f4a4464"
