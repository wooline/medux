{
  "name": "@medux/react",
  "version": "1.1.1-alpha.9",
  "description": "The practice of @medux/core in react.",
  "repository": "https://github.com/wooline/medux/tree/master/packages/react",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "jsnext:main": "dist/next/index.js",
  "types": "types/",
  "files": [
    "env/",
    "types/",
    "dist/"
  ],
  "scripts": {
    "type": "tsc",
    "typeSrc": "tsc -P ./src",
    "eslint": "eslint --cache .",
    "eslintSrc": "eslint --cache ./src",
    "buildTypes": "tsc --project ./src/tsconfig.types.json && copyfiles -f ./src/*.d.ts ./types",
    "prebuild": "node ../../preBuild && yarn type & yarn typeSrc & yarn eslint & yarn eslintSrc && rimraf ./dist ./types && yarn buildTypes",
    "build:next": "cross-env NODE_TAG=next babel ./src --out-dir ./dist/next  --extensions \".ts,.tsx\"",
    "build:esm": "cross-env NODE_TAG=esm babel ./src --out-dir ./dist/esm  --extensions \".ts,.tsx\"",
    "build:cjs": "cross-env NODE_TAG=cjs babel ./src --out-dir ./dist/cjs  --extensions \".ts,.tsx\"",
    "build": "yarn build:next & yarn build:esm & yarn build:cjs",
    "docs-md": "typedoc --plugin typedoc-plugin-markdown  --out api src/index.tsx --hideSources --theme markdown && node fixDocs",
    "eslintConfig": "eslint --print-config /Users/zy/work/medux/medux/packages/react/src/index.tsx > eslintConfig.txt"
  },
  "peerDependencies": {
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-redux": "^7.0.0"
  },
  "dependencies": {
    "@babel/runtime": "~7.12.5",
    "@medux/core": "^1.1.1-alpha.9"
  },
  "gitHead": "c2d8bb9ed8d10102413bdf655c7326e94f4a4464"
}
