{
  "name": "@medux/react",
  "version": "1.0.6",
  "description": "The practice of @medux/core in react.",
  "repository": "https://github.com/wooline/medux/tree/master/packages/react",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "jsnext": "dist/next/index.js",
  "tsSource": "src/",
  "types": "types/",
  "files": [
    "src/",
    "types/",
    "dist/"
  ],
  "scripts": {
    "prebuild": "yarn lint && rimraf ./dist ./types && yarn types",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "types": "tsc --project ./types.tsconfig.json && copyfiles -f ./src/*.d.ts ./types",
    "docs-md": "typedoc --plugin typedoc-plugin-markdown  --out api src/index.tsx --hideSources --theme markdown && node fixDocs",
    "build:next": "cross-env NODE_ENV=production NODE_TAG=next babel ./src --out-dir ./dist/next  --extensions \".ts,.tsx\"",
    "build:esm": "cross-env NODE_ENV=production NODE_TAG=esm babel ./src --out-dir ./dist/esm  --extensions \".ts,.tsx\"",
    "build:cjs": "cross-env NODE_ENV=production NODE_TAG=cjs babel ./src --out-dir ./dist/cjs  --extensions \".ts,.tsx\"",
    "build": "yarn build:next & yarn build:esm & yarn build:cjs"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.0.0",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@medux/core": "^1.0.5",
    "@types/react-redux": "~7.1.7",
    "react-redux": "~7.2.0"
  },
  "gitHead": "c2d8bb9ed8d10102413bdf655c7326e94f4a4464"
}
