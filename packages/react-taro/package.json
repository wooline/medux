{
  "name": "@medux/react-taro",
  "version": "2.0.0-alpha.3",
  "description": "The practice of @medux/core in taro",
  "repository": "https://github.com/wooline/medux/tree/master/packages/react-taro",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "dist/cjs/index.js",
  "tsSource": "src/",
  "types": "types/",
  "files": [
    "src/",
    "env/",
    "types/",
    "dist/"
  ],
  "scripts": {
    "prebuild": "yarn lint && rimraf ./dist ./types && yarn types",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "types": "tsc --project ./types.tsconfig.json && copyfiles -f ./src/*.d.ts ./types",
    "build": "cross-env NODE_ENV=development NODE_TAG=pkg rollup -c & cross-env NODE_ENV=production NODE_TAG=pkg rollup -c"
  },
  "externals": {
    "@tarojs/taro": true,
    "react": true,
    "react-redux": true
  },
  "peerDependencies": {
    "@tarojs/taro": "^3.0.0",
    "@types/react": "^16.8.0",
    "@types/react-redux": "^7.0.0",
    "react": "^16.8.0",
    "react-redux": "^7.0.0"
  },
  "dependencies": {
    "@babel/runtime": "~7.12.5",
    "@medux/core": "^2.0.0-alpha.3",
    "@medux/mini-program": "^2.0.0-alpha.3"
  },
  "gitHead": "c2d8bb9ed8d10102413bdf655c7326e94f4a4464"
}
