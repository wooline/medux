{
  "name": "@medux/react-web-router",
  "version": "0.1.1",
  "description": "The practice of @medux/core in react",
  "repository": "https://github.com/wooline/medux/tree/master/packages/react-web-router",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "tsSource": "src/",
  "types": "types/",
  "files": [
    "src/",
    "types/",
    "dist/",
    "index.js"
  ],
  "scripts": {
    "prebuild": "yarn lint && rimraf ./dist ./types && yarn types",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "types": "tsc && tsc --emitDeclarationOnly --declaration --isolatedModules false --noEmit false  --outDir ./types && copyfiles -f ./src/*.d.ts ./types",
    "build:cjs": "cross-env NODE_ENV=cjs babel ./src --out-dir ./dist/cjs  --extensions \".ts,.tsx\" --source-maps",
    "build:es": "cross-env NODE_ENV=es babel ./src --out-dir ./dist/es  --extensions \".ts,.tsx\" --source-maps",
    "build": "yarn build:cjs & yarn build:es"
  },
  "peerDependencies": {
    "@types/react-dom": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@medux/react": "^0.1.1",
    "@medux/web": "^0.1.1",
    "@types/react-router-dom": "~4.3.4",
    "react-router-dom": "~5.0.1"
  },
  "gitHead": "d72a594493f4a076297491bcc925e9b7fcfbbe37"
}
