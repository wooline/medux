{
  "name": "@medux/react-web-router",
  "version": "0.2.28",
  "description": "The practice of @medux/core in react.",
  "repository": "https://github.com/wooline/medux/tree/master/packages/react-web-router",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "main.js",
  "module": "dist/esm/index.js",
  "jsnext": "dist/next/index.js",
  "tsSource": "src/",
  "types": "types/",
  "files": [
    "src/",
    "types/",
    "dist/",
    "main.js"
  ],
  "scripts": {
    "prebuild": "yarn lint && rimraf ./dist ./types && yarn types",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "types": "tsc --emitDeclarationOnly --declaration --isolatedModules false --noEmit false  --outDir ./types && copyfiles -f ./src/*.d.ts ./types",
    "build:next": "cross-env NODE_ENV=next babel ./src --out-dir ./dist/next  --extensions \".ts,.tsx\"",
    "build:esm": "cross-env NODE_ENV=esm babel ./src --out-dir ./dist/esm  --extensions \".ts,.tsx\"",
    "build:cjs": "cross-env NODE_ENV=cjs rollup -c",
    "build": "yarn build:next & yarn build:esm & yarn build:cjs"
  },
  "externals": {
    "@babel/runtime": true,
    "react": true,
    "react-dom": true,
    "react-redux": true,
    "history": true,
    "redux": true
  },
  "peerDependencies": {
    "@babel/runtime": "^7.0.0",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@medux/core": "^0.2.28",
    "@medux/react": "^0.2.28",
    "@medux/route-plan-a": "^0.2.28",
    "@medux/web": "^0.2.28"
  },
  "gitHead": "c2d8bb9ed8d10102413bdf655c7326e94f4a4464"
}
