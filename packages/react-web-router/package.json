{
  "name": "@medux/react-web-router",
  "version": "2.0.0-beta.3.2",
  "description": "The practice of @medux/core in react.",
  "repository": "https://github.com/wooline/medux/tree/master/packages/react-web-router",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "index.js",
  "module": "main.js",
  "jsnext:main": "dist/next/index.js",
  "unpkg": "dist/umd/index.js",
  "types": "types/",
  "files": [
    "env/",
    "types/",
    "dist/",
    "main.js"
  ],
  "scripts": {
    "type": "tsc",
    "typeSrc": "tsc -P ./src",
    "eslint": "eslint --cache .",
    "eslintSrc": "eslint --cache ./src",
    "buildTypes": "tsc --project ./src/tsconfig.types.json && copyfiles -f ./src/*.d.ts ./types",
    "prebuild": "node ../../preBuild && yarn type & yarn typeSrc & yarn eslint & yarn eslintSrc && rimraf ./dist ./types && yarn buildTypes",
    "build:pkg": "cross-env NODE_TAG=pkg rollup -c & cross-env NODE_ENV=production NODE_TAG=pkg rollup -c",
    "build:next": "cross-env NODE_TAG=next rollup -c",
    "build:esm": "cross-env NODE_TAG=esm rollup -c",
    "build:cjs": "cross-env NODE_TAG=esm NODE_ENV=production rollup -c",
    "build": "yarn build:next & yarn build:esm & yarn build:cjs",
    "docs-md": "typedoc --plugin typedoc-plugin-markdown  --out api src/index.tsx --hideSources --theme markdown && node fixDocs"
  },
  "externals": {
    "react": true,
    "react-dom": true,
    "react-redux": true
  },
  "peerDependencies": {
    "@types/react-redux": "~7.0.0",
    "react-redux": "~7.0.0"
  },
  "dependencies": {
    "@medux/core": "^2.0.0-beta.3.2",
    "@medux/route-plan-a": "^2.0.0-beta.3.2",
    "@medux/web": "^2.0.0-beta.3.2"
  },
  "gitHead": "5638ad91c6d59f14b7083b832629db5eca9195ef"
}
