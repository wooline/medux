{
  "name": "@medux/react-web-router",
  "version": "0.2.17",
  "description": "The practice of @medux/core in react.",
  "repository": "https://github.com/wooline/medux/tree/master/packages/react-web-router",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "tsSource": "src/",
  "types": "types/",
  "files": [
    "src/",
    "types/",
    "dist/",
    "index.js"
  ],
  "scripts": {
    "prebuild": "yarn lint && rimraf ./dist ./types && yarn types",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "types": "tsc --emitDeclarationOnly --declaration --isolatedModules false --noEmit false  --outDir ./types && copyfiles -f ./src/*.d.ts ./types",
    "build:cjs": "cross-env NODE_ENV=cjs babel ./src --out-dir ./dist/cjs  --extensions \".ts,.tsx\" --source-maps",
    "build:esm": "cross-env NODE_ENV=esm babel ./src --out-dir ./dist/esm  --extensions \".ts,.tsx\" --source-maps",
    "build:es6": "cross-env NODE_ENV=es6 babel ./src --out-dir ./dist/es6  --extensions \".ts,.tsx\" --source-maps",
    "build": "yarn build:esm & yarn build:cjs & yarn build:es6"
  },
  "peerDependencies": {
    "@types/react-dom": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@medux/react": "^0.2.17",
    "@medux/route-plan-a": "^0.2.17",
    "@medux/web": "^0.2.17",
    "@types/react-router-dom": "~5.1.2",
    "react-router-dom": "~5.1.2"
  },
  "gitHead": "c2d8bb9ed8d10102413bdf655c7326e94f4a4464"
}
