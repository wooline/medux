{
  "name": "@medux/react-web-router",
  "version": "1.1.1-alpha.2",
  "description": "The practice of @medux/core in react.",
  "repository": "https://github.com/wooline/medux/tree/master/packages/react-web-router",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "main.js",
  "module": "dist/esm/index.js",
  "jsnext": "dist/next/index.js",
  "tsSource": "src/",
  "types": "types/",
  "files": [
    "src/",
    "env/",
    "types/",
    "dist/",
    "main.js"
  ],
  "scripts": {
    "type": "tsc",
    "typeSrc": "tsc -P ./src",
    "eslint": "eslint --cache .",
    "eslintSrc": "eslint --cache ./src",
    "buildTypes": "tsc --project ./src/tsconfig.types.json && copyfiles -f ./src/*.d.ts ./types",
    "prebuild": "yarn type & yarn typeSrc & yarn eslint & yarn eslintSrc && rimraf ./dist ./types && yarn buildTypes",
    "build:next": "cross-env NODE_ENV=production NODE_TAG=next babel ./src --out-dir ./dist/next  --extensions \".ts,.tsx\"",
    "build:esm": "cross-env NODE_ENV=production NODE_TAG=esm babel ./src --out-dir ./dist/esm  --extensions \".ts,.tsx\"",
    "build:pkg": "cross-env NODE_ENV=development NODE_TAG=pkg rollup -c & cross-env NODE_ENV=production NODE_TAG=pkg rollup -c",
    "build": "yarn build:next & yarn build:esm & yarn build:pkg",
    "docs-md": "typedoc --plugin typedoc-plugin-markdown  --out api src/index.tsx --hideSources --theme markdown && node fixDocs"
  },
  "externals": {
    "react": true,
    "react-dom": true,
    "react-redux": true,
    "history": true
  },
  "peerDependencies": {
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-redux": "^7.0.0"
  },
  "dependencies": {
    "@babel/runtime": "~7.9.0",
    "@medux/core": "^1.1.1-alpha.2",
    "@medux/react": "^1.1.1-alpha.2",
    "@medux/route-plan-a": "^1.1.1-alpha.2",
    "@medux/web": "^1.1.1-alpha.2"
  },
  "gitHead": "c2d8bb9ed8d10102413bdf655c7326e94f4a4464"
}
