{
  "name": "@medux/core",
  "version": "0.0.48",
  "description": "",
  "repository": "https://github.com/wooline/medux/tree/master/packages/core",
  "homepage": "https://github.com/wooline/medux/blob/master/packages/core/npm.md",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "index.js",
  "tsSource": "src/",
  "types": "types/",
  "files": [
    "src/",
    "types/",
    "dist/",
    "index.js"
  ],
  "scripts": {
    "prebuild": "yarn lint && rimraf ./dist ./types && yarn types",
    "lint": "eslint \"./src/**/*.ts\"",
    "types": "tsc && tsc --emitDeclarationOnly --declaration --isolatedModules false --noEmit false --outDir ./types && copyfiles -f ./src/*.d.ts ./types",
    "build:prod": "cross-env NODE_ENV=production babel ./src --out-dir ./dist/prod  --extensions \".ts,.tsx\" --source-maps",
    "build:dev": "cross-env NODE_ENV=development babel ./src --out-dir ./dist/dev  --extensions \".ts,.tsx\" --source-maps",
    "build": "yarn build:dev & yarn build:prod"
  },
  "browserslist": [
    "> 1%"
  ],
  "peerDependencies": {
    "@babel/runtime": "^7.0.0",
    "core-js": "^3.0.0"
  },
  "dependencies": {
    "redux": "~4.0.1"
  },
  "gitHead": "ea886e4118516642fb243fb569f5f754d97e18ba"
}
