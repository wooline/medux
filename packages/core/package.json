{
   "name": "@medux/core",
   "version": "0.0.8",
   "description": "",
   "main": "index.js",
   "types": "./types",
   "files": [
      "dist/",
      "types/",
      "index.js"
   ],
   "scripts": {
      "prebuild": "yarn lint && rimraf ./dist ./types && yarn types",
      "lint": "eslint \"./src/**/*.ts\"",
      "types": "tsc && tsc --emitDeclarationOnly --declaration --isolatedModules false --noEmit false && copyfiles -f ./src/*.d.ts ./types",
      "build:lib": "cross-env NODE_ENV=library babel ./src --out-dir ./dist/lib  --extensions \".ts,.tsx\" --source-maps",
      "build:prod": "cross-env NODE_ENV=production babel ./src --out-dir ./dist/prod  --extensions \".ts,.tsx\" --source-maps",
      "build:dev": "cross-env NODE_ENV=development babel ./src --out-dir ./dist/dev  --extensions \".ts,.tsx\" --source-maps",
      "build": "yarn build:dev & yarn build:prod & yarn build:lib"
   },
   "browserslist": [
      "ie >= 11"
   ],
   "publishConfig": {
      "access": "public"
   },
   "author": "",
   "license": "MIT",
   "dependencies": {
      "@babel/runtime": "^7.4.5",
      "core-js": "^3.1.3",
      "redux": "^4.0.1"
   },
   "gitHead": "4f3a36fcc1f8c9eabeb07d491572b5e2c3e2f496"
}
