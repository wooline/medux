{
  "name": "@medux/core",
  "version": "0.1.3",
  "description": "Modular state management, Suitable for various javascript frameworks",
  "repository": "https://github.com/wooline/medux/tree/master/packages/core",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "keywords": [
    "redux",
    "typescript",
    "state container",
    "state management"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "tsSource": "src/",
  "types": "types/",
  "files": [
    "src/",
    "types/",
    "dist/",
    "index.js"
  ],
  "scripts": {
    "prebuild": "yarn lint && rimraf ./dist ./types && yarn types",
    "lint": "eslint \"./src/**/*.ts\"",
    "types": "tsc && tsc --emitDeclarationOnly --declaration --isolatedModules false --noEmit false --outDir ./types && copyfiles -f ./src/*.d.ts ./types",
    "build:es": "cross-env NODE_ENV=es babel ./src --out-dir ./dist/es  --extensions \".ts,.tsx\" --source-maps",
    "build:cjs": "cross-env NODE_ENV=cjs babel ./src --out-dir ./dist/cjs  --extensions \".ts,.tsx\" --source-maps",
    "build": "yarn build:es & yarn build:cjs"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.0.0",
    "core-js": "^3.0.0"
  },
  "dependencies": {
    "redux": "~4.0.1"
  },
  "gitHead": "c2d8bb9ed8d10102413bdf655c7326e94f4a4464"
}
